<%= simple_form_for(@email,:class=>"form-horizontal") do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :email %>
    <%= f.association :customer %>
  </div>
  <div class="form-group">
    <%= f.button :submit ,:class=>"btn btn-primary"%>
    <%= link_to 'Back', customers_path,:class=>"btn btn-primary" %>
  </div>
<% end %>
